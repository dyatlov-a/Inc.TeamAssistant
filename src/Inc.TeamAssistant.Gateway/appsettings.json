{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Inc.TeamAssistant": "Information"
    }
  },
  "AllowedHosts": "*",
  "AddStoryOptions": {
    "LinksPrefix": ["http://", "https://"]
  },
  "TelegramBotOptions": {
    "CacheAbsoluteExpiration": "00:05:00",
    "ConnectToSessionLinkTemplate": "{0}?start={1}"
  },
  "ReviewerOptions": {
    "LinkForConnectTemplate": "{0}?start={1}",
    "NotificationsDelay": "00:01:00",
    "NotificationsBatch": 10,
    "Workday": {
      "WorkOnHoliday": false,
      "StartTimeUtc": "07:00:00",
      "EndTimeUtc": "16:00:00",
      "NotificationInterval": "00:30:00"
    }
  },
  "HolidayOptions": {
    "CacheTimeout": "00:30:00"
  }
}